<DashboardScreen>:
    name: 'dashboard'
    MDBottomNavigation:
        panel_color: "#FFFDD0"
        selected_color_background: "darkkhaki"
        text_color_active: "lightgray"
        MDBottomNavigationItem:
            name:"HomeScreen1"
            icon:'home'
            md_bg_color: 255/255, 253/255, 221/255, 1

            MDScreen:
                MDBoxLayout:
                    orientation:'vertical'
                    padding:'20dp'
                    MDBoxLayout:
                        size_hint_y:.10
                        padding:dp(10)

                    MDIconButton:
                        icon:"images/userconf.png"
                        size_hint: .3, .15
                        pos_hint: {"center_x": .1, "center_y": .1}
                        on_release: app.root.current = 'aboutuser'

                    MDBoxLayout:
                        size_hint_y:.25
                        padding:dp(25)
                        MDBoxLayout:
                            MDLabel:
                                text:" "
                                halign: "center"
                                font_size: "22sp"
                                font_name: "Teachers-SemiBold.ttf"
                                pos_hint: {"center_x": .1, "center_y": .90}
                                id: lbdashboard

                    MDGridLayout:
                        size_hint_y:.500
                        cols:2
                        padding:[dp(10),dp(0),dp(10),dp(20)]
                        spacing:dp(20)
                        ElementCard:
                            image:"images/registo_medico.png"
                            text:"Salvar Treinos"
                            on_release: app.root.current = 'precheck'

                        ElementCard:
                            image:"images/ginastica.png"
                            text:"Estoque"
                            on_release: app.root.current = 'meds'

                        ElementCard:
                            image:"images/dieta.png"
                            text:"Dia de Treino"
                            on_release: app.root.current = 'activities'
                            adaptive_height:False

                        ElementCard:
                            image:"images/cronometro.png"
                            text:"Informações"
                            on_release: app.root.current = 'info'
                            adaptive_height:False


        MDBottomNavigationItem:
            name:"HomeScreen2"
            icon:'lock'
            disabled: False
            md_bg_color: 255/255, 253/255, 221/255, 1

            MDScreen:
                ScrollView:
                    bar_width:0
                    MDBoxLayout:
                        orientation:'vertical'
                        padding:'20dp'
                        MDLabel:
                            text:''
                            font_style:"H2"
                            adaptive_height:True
                        MDBoxLayout:
                            size_hint_y:.1
                            padding:dp(1)
                            MDBoxLayout:
                        MDGridLayout:
                            size_hint_y:.500
                            cols:2
                            padding:[dp(10),dp(10),dp(10),dp(200)]
                            spacing:dp(20)
                            ElementCard:
                                image:"images/musculo.png"
                                text:"Lista Personal"
                                on_release: app.root.current = 'prechec'

                            ElementCard:
                                image:"images/dieta_2.png"
                                text:"Dietas"
                                on_release: app.root.current = 'activitie'
                                adaptive_height:False


<ElementCard@MDCard>:
    md_bg_color: 196/255, 195/255, 171/255, 1
    padding:dp(10)
    spacing:dp(20)
    radius:dp(15)
    ripple_behavior:True
    image:''
    text:""
    items_count:""
    subtext:''

    orientation:'vertical'
    Image:
        source:root.image
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            halign:"center"
            text:root.text
            font_size: "15sp"
            font_name: "Teachers-Medium.ttf"
        MDLabel:
            halign:"center"
            font_style:"Caption"
            text:root.subtext
        MDLabel:
            halign:"center"
            text:root.items_count

